{
    "name": "lotgd-core/clan-news-bundle",
    "type": "lotgd-bundle",
    "description": "Clan News for LoTGD IDMarinas Edition.",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Iv√°n Diaz Marinas (IDMarinas)",
            "email": "35842929+idmarinas@users.noreply.github.com",
            "homepage": "https://github.com/lotgd-core/clan-news-bundle",
            "role": "Developer, Maintainer"
        },
        {
            "name": "dying",
            "role": "Original Idea"
        }
    ],
    "support": {
        "issues": "https://github.com/lotgd-core/clan-news-bundle/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 | ^8.0",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/http-kernel": "^4.4"
    },
    "require-dev": {
        "idmarinas/lotgd": "^5.2|^6.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "nyholm/symfony-bundle-test": "^1.7",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/test-pack": "^1.0"
    },
    "conflict": {
        "idmarinas/lotgd": "<5.2"
    },
    "autoload": {
        "psr-4": {
            "Lotgd\\Bundle\\ClanNewsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lotgd\\Bundle\\ClanNewsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php bin/phpunit --process-isolation"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 5000,
        "sort-packages": true
    },
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.me/idmarinas"
        }
    ]
}
